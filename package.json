{
	"name": "msgcrypter",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "npm run prepare && svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npm run prepare && prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "^1.0.0-next.320",
		"@tailwindcss/forms": "^0.5.0",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.7",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"html-minifier": "^4.0.0",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.4.0",
		"typescript": "~4.6.3"
	},
	"type": "module",
	"dependencies": {
		"@macfja/svelte-persistent-store": "^1.3.0",
		"idb-keyval": "^6.1.0"
	}
}
